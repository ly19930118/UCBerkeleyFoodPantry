<div class="trans-logo-background inventory-page">
  <div class="row">
    <div class="about-images col-xs-6">
      <%= image_tag("food3.jpg", class:'image') %>
      <%= image_tag("food1.jpg", class:'image') %>
    </div>
    <div class="inventory-info col-xs-6">
      <h2>Inventory Form</h2>
    <form role="form">
      <div class="form-group">
        <label for="email">Email:</label>
        <input type="email" class="form-control" id="email" placeholder="Enter email">
      </div>
      <div class="form-group">
        <label for="pwd">Field1:</label>
        <input type="password" class="form-control" id="pwd" placeholder="Enter password">
      </div>
      <div class="checkbox">
        <label><input type="checkbox">Field 2:</label>
      </div>
      <button type="submit" class="btn btn-default">Submit</button>
    </form>
    </div>
  </div>

</div>

