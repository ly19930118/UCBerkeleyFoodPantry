<div class="alignCarousal">
    <h2>Resources</h2>
    <div id="carousel-example-generic" class="carousel slide" data-ride="carousel">
      <!-- Indicators -->
      <ol class="carousel-indicators">
        <li data-target="#carousel-example-generic" data-slide-to="0" class="active"></li>
        <li data-target="#carousel-example-generic" data-slide-to="1"></li>
        <li data-target="#carousel-example-generic" data-slide-to="2"></li>
        <li data-target="#carousel-example-generic" data-slide-to="3"></li>
        <li data-target="#carousel-example-generic" data-slide-to="4"></li>
        <li data-target="#carousel-example-generic" data-slide-to="5"></li>
        <li data-target="#carousel-example-generic" data-slide-to="6"></li>
        <li data-target="#carousel-example-generic" data-slide-to="7"></li>
        <li data-target="#carousel-example-generic" data-slide-to="8"></li>
        <li data-target="#carousel-example-generic" data-slide-to="9"></li>
        <li data-target="#carousel-example-generic" data-slide-to="10"></li>
      </ol>

      <!-- Wrapper for slides -->
      <div class="carousel-inner" role="listbox">
        <div class="item active">
          <%= image_tag("finaid.jpg", :alt => "finaid", width: '75%', height: '75%') %>
          <div class="carousel-caption">
            <h3>Financial Aid & Scholarships Office</h3>
            <p><a>http://financialaid.berkeley.edu/food-resources</a></p>
          </div>
        </div>
        <div class="item">
          <%= image_tag("ce3.jpg", :alt => "ce3", width: '75%', height: '75%') %>
          <div class="carousel-caption">
            <h3>Centers for Educational Equity & Excellence</h3>
            <p><a>http://ce3.berkeley.edu/foodsecurity/</a></p>
          </div>
        </div>
        <div class="item">
          <%= image_tag("foodInst.jpeg", :alt => "foodinst", width: '75%', height: '75%') %>
          <div class="carousel-caption">
            <h3>Berkeley Food Institute</h3>
            <p><a>http://food.berkeley.edu/</a></p>
          </div>
        </div>
        <div class="item">
          <%= image_tag("studentFood.jpg", :alt => "studfood", width: '75%', height: '75%') %>
          <div class="carousel-caption">
            <h3>Berkeley Student Food Collective</h3>
            <p><a>http://berkeleystudentfoodcollective.org/</a></p>
          </div>
        </div>
        <div class="item">
          <%= image_tag("foodAssis.jpg", :alt => "foodassis", width: '75%', height: '75%') %>
          <div class="carousel-caption">
            <h3>Food Assistance Program</h3>
            <p><a>http://financialaid.berkeley.edu/food-assistance-program</a></p>
          </div>
        </div>
        <div class="item">
          <%= image_tag("caldining.jpg", :alt => "caldin", width: '75%', height: '75%') %>
          <div class="carousel-caption">
            <h3>Cal Dining</h3>
            <p><a>http://caldining.berkeley.edu/</a></p>
          </div>
        </div>
        <div class="item">
          <%= image_tag("healthTang.jpg", :alt => "tang", width: '75%', height: '75%') %>
          <div class="carousel-caption">
            <h3>"Health Matters" University Health Services</h3>
            <p><a>http://uhs.berkeley.edu/facstaff/healthmatters/nutrition.shtml</a></p>
          </div>
        </div>
        <div class="item">
          <%= image_tag("globalFood.png", :alt => "global", width: '75%', height: '75%') %>
          <div class="carousel-caption">
            <h3>University of California Global Food Initiative</h3>
            <p><a>http://universityofcalifornia.edu/global-food-initiative</a></p>
          </div>
        </div>
        <div class="item">
          <%= image_tag("organicGard.jpg", :alt => "organic", width: '75%', height: '75%') %>
          <div class="carousel-caption">
            <h3>Student Organic Garden Association</h3>
            <p><a>http://berksoga.tumblr.com/</a></p>
          </div>
        </div>
        <div class="item">
          <%= image_tag("gilltract.jpg", :alt => "gill", width: '75%', height: '75%') %>
          <div class="carousel-caption">
            <h3>Gill Tract Community Farm</h3>
            <p><a>https://gilltractfarm.wordpress.com/</a></p>
          </div>
        </div>
      </div>

      <!-- Controls -->
      <a class="left carousel-control" href="#carousel-example-generic" role="button" data-slide="prev">
        <span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
        <span class="sr-only">Previous</span>
      </a>
      <a class="right carousel-control" href="#carousel-example-generic" role="button" data-slide="next">
        <span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span>
        <span class="sr-only">Next</span>
      </a>
    </div>
</div>